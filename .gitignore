application.properties
application.local.properties
application.prod.properties
application.dev.properties

# External configuration files
*.config
*.properties.local
*.properties.secrets

# Environment files
.env
.env.local
.env.production
.env.development
.env.test

# Credentials files
*.credentials
*.secrets
*.passwords
auth.json

